{
  "compilerOptions": {
    "incremental": true,
    "target": "esnext",
    "experimentalDecorators": true,
    "module": "commonjs",
    "noEmit": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true
  }
}
